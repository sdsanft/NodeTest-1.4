<head>
	<title>Meeting Scheduler</title>
</head>
<body>
    <h1>Project Liger</h1>
    <p>Hello, <%= settings.meetings[id].clientName%>!</p>

    <form action="http://localhost:<%= port%>/meeting/<%= settings.username%>/<%= id%>" method="POST">
        Date <input type="date" name="date"> <br>
        <% for (var time = settings.meetingInfo.timeIn; time < settings.meetingInfo.timeOut; time += 0.5) { %>
            <input type="radio" value="<%= time%>" name="time"> <%=time%> <br>
        <% } %>
        <input type="submit" value="submit">        
    </form>
  
    <a href="http://localhost:<%= port%>">Home</a>
</body>